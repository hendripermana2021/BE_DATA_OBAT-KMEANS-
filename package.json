{
  "name": "be-smom",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize db:create",
    "dev": "nodemon ./index.js",
    "start": "node ./index.js",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:restart": "npx sequelize-cli db:drop db:create db:migrate db:seed",
    "tbl_users": "npx sequelize-cli model:generate --name tbl_users --attributes name_user:string,sex:string,email:string,password:string,real_password:string,role_id:string,accesstoken:text,refreshtoken:text",
    "tbl_alternatif": "npx sequelize-cli model:generate --name tbl_alternatif --attributes name_obat:string,cluster:string,qty_barang:integer,harga_barang:string,detail_penyembuhan:string,detail_efek:string",
    "tbl_kriteria": "npx sequelize-cli model:generate --name tbl_kriteria --attributes name_kriteria:string",
    "tbl_subkriteria": "npx sequelize-cli model:generate --name tbl_subkriteria --attributes name_sub:string,value:float,id_kriteria:integer",
    "tbl_alternatif_value": "npx sequelize-cli model:generate --name tbl_alternatif_value --attributes id_alternatif:integer,id_kriteria:integer,id_subkriteria:integer",
    "tbl_centroid": "npx sequelize-cli model:generate --name tbl_centroid --attributes id_centroid:integer,name_cluster:integer",
    "seed-users": "npx sequelize-cli seed:generate --name demo-tbl_users",
    "seed-alternatif": "npx sequelize-cli seed:generate --name demo-tbl_alternatif",
    "seed-kriteria": "npx sequelize-cli seed:generate --name demo-tbl_kriteria",
    "seed-subkriteria": "npx sequelize-cli seed:generate --name demo-tbl_subkriteria",
    "seed-alternatif_value": "npx sequelize-cli seed:generate --name demo-tbl_alternatif_value",
    "seed-centroid": "npx sequelize-cli seed:generate --name demo-tbl_centroid"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "babel-loader": "^9.1.3",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.0",
    "nodemon": "^3.1.3",
    "path": "^0.12.7",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  }
}
